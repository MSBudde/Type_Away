<% current_user.posts.each do |post| -%>
    <div class="container">
      <div class="contain1">
        <h1><%= post.title%></h1>
        <div class="contain1">
          <p><%= post.content%></p>
        </div>
      </div>
      <%= link_to "Delete this post", post_path(post), method: :delete, data: { confirm: "Are you sure?"} %>
  
    </div>


  <% end -%>
