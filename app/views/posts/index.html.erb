<% @posts.each do |post| %>
  <%= link_to post_path(post.id) do %>
    <div class="contain1">
      <h1 class="postTitle"><%= post.title %></h1>

      </br>
        <% if current_user %>
        <% if post.user == current_user %>
        <p class="text-right">
          <%= link_to "Delete this post", post_path(post), method: :delete, data: { confirm: "Are you sure?"} %>
        </p>
        <% end %>
        <% end %>
      </div>
      </div>
      <% end %>
      <% end %>
